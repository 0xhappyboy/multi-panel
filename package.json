{
  "name": "multi-panel",
  "version": "1.2.0",
  "description": "A front-end component that supports dynamic area segmentation and nesting of multiple panels.",
  "license": "Apache2.0",
  "author": "happyboy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xhappyboy/multi-panel.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "files": [
    "dist"
  ],
  "keywords": [
    "multi panel",
    "panel split"
  ],
  "type": "module",
  "microbundle": {
    "entries": [
      "src/index.ts"
    ],
    "peerDependencies": [
      "react",
      "react-dom"
    ],
    "externals": [
      "react",
      "react-dom",
      "react/jsx-runtime"
    ],
    "generateTypes": true,
    "css": false
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "microbundle-crl --no-compress --format modern,cjs"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup": "^4.53.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.9.3",
    "microbundle-crl": "^0.13.11"
  },
  "bugs": {
    "url": "https://github.com/0xhappyboy/multi-panel/issues"
  },
  "homepage": "https://github.com/0xhappyboy/multi-panel#readme"
